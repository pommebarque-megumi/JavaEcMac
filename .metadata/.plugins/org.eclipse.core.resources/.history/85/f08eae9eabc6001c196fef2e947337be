package pc;

public class PcApp {

	public static void main(String[] args) {
		Pc pc=new Pc();
		pc.setUsb(new USBMemory());
		pc.executeUSB();
		pc.setUsb(new USBFan());
		pc.executeUSB();
	}

}
class USBMemory implements IUSB{
	@Override
	public void execute() {
		System.out.println("SAVE THE DATA");
	}
}
class USBFan implements IUSB{
	@Override
	public  void execute() {
		System.out.println("Bhoooon...");
	}
}